generator client {
  provider = "prisma-client-js"
  engineType = "binary"
  binaryTargets = ["native", "darwin-arm64"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Model de test pour la génération du client
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}